export PATH="$PATH:/opt/homebrew/bin"
export PATH="/opt/homebrew/opt/gnu-sed/libexec/gnubin:$PATH" # gsed as sed on m1
export PATH="/usr/local/opt/gnu-sed/libexec/gnubin:$PATH" # gsed as sed on intel
export PATH="$ANDROID_HOME/platform-tools:$PATH"
